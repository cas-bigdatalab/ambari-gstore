{
  "layouts": [
    {
      "layout_name": "default_gstore_dashboard",
      "display_name": "Standard GStore Dashboard",
      "section_name": "GSTORE_SUMMARY",
      "widgetLayoutInfo": [
        {
          "widget_name": "Triples",
          "description": "Number of Triples",
          "widget_type": "NUMBER",
          "is_visible": true,
          "metrics": [
            {
              "name": "triple",
              "metric_path": "metrics/gstore/triple",
              "service_name": "GSTORE",
              "component_name": "GSTOREMS"
            }
          ],
          "values": [
            {
              "name": "Number of Triples",
              "value": "${triple/1000000.0}"
            }
          ],
          "properties": {
             "display_unit": "M"
          }
        },
		 {
